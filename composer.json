{
    "name": "forumhulp/forumtopicwatch",
    "type": "phpbb-extension",
    "description": "Watch users and their subscribed forums / topics.",
    "homepage": "http://forumhulp.com/",
    "version": "3.1.2",
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "John Peskens",
            "homepage": "http://forumhulp.com/",
            "email": "info@forumhulp.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3",
        "phpbb/phpbb": "3.1.*@dev"
    },
    "extra": {
        "display-name": "Forum- topicwatch",
		"soft-require": {
           "phpbb/phpbb": ">3.2.*@dev"
        },
		"version-check": {
			"host": "forumhulp.com",
			"directory": "/versioncheck",
			"filename": "forum-topicwatch"
		}
    }
}
